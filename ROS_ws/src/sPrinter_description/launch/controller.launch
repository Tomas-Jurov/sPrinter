<launch>

<rosparam file="$(find sPrinter_description)/launch/controller.yaml" command="load"/> 
<node name="controller_spawner" ns="/sPrinter" pkg="controller_manager" type="spawner" respawn="false" output="screen" args="Boggie_right_position_controller Boggie_left_position_controller Wheel_right_front_position_controller Wheel_right_middle_position_controller Wheel_right_rear_position_controller Wheel_left_front_position_controller Wheel_left_middle_position_controller Wheel_left_rear_position_controller MainFrame_pitch_position_controller Lens_Y_axis_trans_position_controller Lens_X_axis_trans_position_controller Lens_Y_axis_rot_position_controller Lens_X_axis_rot_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/sPrinter/joint_states"/>
</node>

</launch>
