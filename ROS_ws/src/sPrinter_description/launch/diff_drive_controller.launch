<launch>

<rosparam file="$(find sPrinter_description)/config/diff_drive.yaml" command="load"/> 
<node name="wheels_controller_spawner" ns="/sPrinter" pkg="controller_manager" type="spawner" respawn="false" output="screen" 
args="
			diff_drive_controller
			joint_state_controller
"/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/sPrinter/joint_states"/>
</node>

<node name="rqt_robot_steering" pkg="rqt_robot_steering" type="rqt_robot_steering">
	<param name="default_topic" value="sPrinter/diff_drive_controller/cmd_vel"/>
</node>

</launch>
